{
  "name": "brevifolia-nextjs",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Next.js",
  "license": "MIT",
  "version": "0.1.0",
  "author": "@kendallstraut",
  "scripts": {
    "dev": "next src",
    "develop": "concurrently \"yarn dev\" \"tina-git-server 3001\"",
    "build": "next build src",
    "start": "concurrently \"next start src -p $PORT\" \"tina-git-server 3001\"",
    "heroku-postbuild": "yarn build",
    "export": "next export src",
    "deploy": "yarn build && yarn export"
  },
  "dependencies": {
    "@tinacms/api-git": "^0.4.6",
    "@tinacms/git-client": "^0.4.1",
    "concurrently": "^5.0.0",
    "gray-matter": "^4.0.2",
    "moment": "^2.24.0",
    "next": "^9.0.2",
    "node-glob": "^1.2.0",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-tinacms": "^0.9.2",
    "styled-components": "^4.4.1",
    "tinacms": "^0.10.0",
    "webpack": "^4.37.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "keywords": [
    "next"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kendallstrautman/brevifolia-nextjs"
  }
}
